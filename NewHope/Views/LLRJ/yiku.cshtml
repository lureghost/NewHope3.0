
@{
    ViewBag.Title = "yiku";
   Layout = "~/Views/New/Index.cshtml";
}

<h2>yiku</h2>

<head>
    @*<link href="~/layui/css/layui.css" rel="stylesheet" />*@
</head>
<body class="layui-layout-body">
    <div class="layui-layout layui-layout-admin" >
      

        <div class="layui-body" >
            <!-- 内容主体区域 -->

            <ul class="">
                <li class=""><a href="">首页</a>>  <a href="">仓库作业</a>>    <a href="">移库管理</a></li>


            </ul>
            <form class="layui-form" action="" lay-filter="example">
                <div>
                    <h3 >移库管理</h3>
                </div>

                <div class="layui-btn-group">
                    <button type="button" class="layui-btn">所有</button>
                    <button type="button" class="layui-btn ">待审核</button>
                    <button type="button" class="layui-btn">审核通过</button>
                    <button type="button" class="layui-btn">审核失败</button>

                </div>
                <div style="float:right">
                    <button type="button" onclick="location.href='/New/addyikudan'" class="layui-btn" ><i class="layui-icon" > </i>新增</button>
                    <button type="button" class="layui-btn"><i class="layui-icon"></i>删除</button>
                    <button type="button" class="layui-btn"><i class="layui-icon"></i>导出</button>
                </div>
              
                <div class="layui-form-item">



                    <div class="layui-inline">
                        <div class="layui-input-inline" style="width: 200px;">
                            <input type="text" name="danhao" lay-verify="title" autocomplete="off" placeholder="请输入报损单号" class="layui-input">
                        </div>
                        <div class="layui-input-inline" style="width: 100px;">
                            <input type="text" name="time_min" placeholder="开始时间" autocomplete="off" class="layui-input">
                        </div>
                        <div class="layui-form-mid">-</div>
                        <div class="layui-input-inline" style="width: 100px;">
                            <input type="text" name="time_max" placeholder="结束时间" autocomplete="off" class="layui-input">
                        </div>
                        <button type="button" class="layui-btn layui-btn-normal">搜索</button>
                    </div>
                    <div class="table">
                        <table class="layui-table">

                            <thead>
                                <tr>
                                    <td><input id="Checkbox1" type="checkbox" /></td>
                                    <td>&nbsp;移库单编号</td>
                                    <td>&nbsp;移库类型</td>
                                    <td>&nbsp;关联单号</td>
                                    <td>&nbsp;移库总数</td>
                                    <td>&nbsp;移库人</td>
                                    <td>&nbsp;状态</td>
                                    <td>&nbsp;创建时间</td>
                                    <td>&nbsp;操作</td>


                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td><input id="Checkbox1" type="checkbox" /></td>
                                    <td>&nbsp;单号1111</td>
                                    <td>&nbsp;移库上架</td>
                                    <td>&nbsp;23</td>
                                    <td>&nbsp;3</td>
                                    <td>&nbsp;admin</td>
                                    <td>&nbsp;等待审核</td>
                                    <td>&nbsp;2020/4/9</td>
                                    <td>&nbsp;新增删除查看审核</td>

                                </tr>
                            </tbody>
                        </table>
                       
                        <div id="demo8"></div>

                    </div>
                   
                </div>



            </form>
            
            </div>


        </div>
        
   

    <script src="~/layui/layui.js"></script>
    <script src="~/layui/layui.all.js"></script>
    <script>
        //JavaScript代码区域
        layui.use(['laypage', 'layer'], function () {
            var laypage = layui.laypage
                , layer = layui.layer;
        laypage.render({
            elem: 'demo8'
            , count: 1000
            , layout: ['limit', 'prev', 'page', 'next']
        });
          
    </script>
</body>