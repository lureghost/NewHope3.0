
@{
    ViewBag.Title = "yikuSH";
}
    <head>
        <link href="~/layui/css/layui.css" rel="stylesheet" />
    </head>
    <body class="layui-layout-body">
        <div class="layui-layout layui-layout-admin">
            <form class="layui-form" id="form1" action="" lay-filter="example">
                <div>
                    <h3>移库审核</h3>
                </div>




                <div class="layui-form-item">



                    <div class="layui-inline">
                        <div class="layui-input-inline" style="width: 200px;">
                            <input type="text" id="id" name="danhao" lay-verify="title" autocomplete="off" placeholder="" class="layui-input">
                        </div>
                        <div class="layui-input-inline" style="width: 100px;">
                            <input type="text" id="startime" name="time_min" placeholder="" autocomplete="off" class="layui-input">
                        </div>

                    </div>
                    <div class="table">
                        <table class="layui-table">

                            <thead>
                                <tr>
                                    <td><input id="allChecks" checked="checked" type="checkbox" /></td>
                                    <td>&nbsp;移库单编号</td>
                                    <td>&nbsp;移库类型</td>
                                    <td>&nbsp;关联单号</td>
                                    <td>&nbsp;移库总数</td>
                                    <td>&nbsp;移库人</td>
                                    <td>&nbsp;状态</td>
                                    <td>&nbsp;创建时间</td>
                                    <td>&nbsp;操作</td>


                                </tr>
                            </thead>
                            <tbody id="tb1">
                            </tbody>
                        </table>

                        <div id="demo8"></div>

                    </div>

                </div>



        </form>
       </div>
    </body>